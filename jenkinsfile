pipeline
{
  agent any
  stage
  {
    stage('scm checkout')
    { stage { git branch: 'master', url: 'https://github.com/sachinydevops/mavenproject-1' }}
  
  
    stage('code comple')
    {steps { withMaven(globalMavenSettingsConfig: '', jdk: 'JDK_HOME', maven: 'MAVEN_HOME', mavenSettingsConfig: '', traceability: true) {
    sh 'mvn compile'
}}}

stage('execute unit test framework')
    {steps { withMaven(globalMavenSettingsConfig: '', jdk: 'JDK_HOME', maven: 'MAVEN_HOME', mavenSettingsConfig: '', traceability: true) {
    sh 'mvn test'
  }}}

}
}
